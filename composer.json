{
    "name": "abowloflrf/",
    "description": "A simple url shortener website using Slim-Framework, MySQL and Memcached",
    "keywords": [
        "url",
        "slim-framework",
        "mysql",
        "memcached"
    ],
    "homepage": "https://github.com/abowloflrf/url-shortener",
    "license": "MIT",
    "authors": [
        {
            "name": "Ruofeng Lei",
            "email": "i@ruofeng.me",
            "homepage": "https://ruofeng.me"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "slim/slim": "^3.1",
        "slim/php-view": "^2.0",
        "monolog/monolog": "^1.17",
        "illuminate/database": "5.5.*",
        "hashids/hashids": "^2.0"
    },
    "require-dev": {},
    "autoload-dev": {
        "classmap": [
            "src/Controllers"
        ]
    },
    "config": {
        "process-timeout": 0
    },
    "scripts": {
        "start": "php -S 127.0.0.1:8080 -t public public/index.php"
    }
}
